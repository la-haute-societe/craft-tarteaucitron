{
  "entry": {
    "settings": "./js/settings.js",
    "tarteaucitron": "./js/tarteaucitron.js"
  },
  "sourceDir": "..\/resources\/",
  "outputDir": "..\/src\/resources",
  "assets": {
    "styles": "css",
    "scripts": "js",
    "copy": [
      {
        "from": "../node_modules/tarteaucitronjs/tarteaucitron.services.js",
        "to": "js/tarteaucitron.services.js"
      },
      {
        "from": "../node_modules/tarteaucitronjs/lang/**/*",
        "to": "js/lang/"
      }
    ]
  },

  "filenames": {
    "development": {
      "js": "[name].js",
      "css": "[name].css",
      "cssChunk": "[id].css"
    },
    "production": {
      "js": "[name].js",
      "css": "[name].css",
      "cssChunk": "[id].css"
    }
  }
}