{
  "entry": {
    "settings": "./js/settings.js"
  },
  "sourceDir": "..\/resources\/",
  "outputDir": "..\/src\/resources/control-panel",
  "assets": {
    "styles": "css",
    "scripts": "js",
    "copy": [
      {
        "from": "../resources/assets/**/*",
        "to": "assets/"
      }
    ]
  },

  "filenames": {
    "development": {
      "js": "[name].js",
      "css": "[name].css",
      "cssChunk": "[id].css"
    },
    "production": {
      "js": "[name].js",
      "css": "[name].css",
      "cssChunk": "[id].css"
    }
  }
}
