{
  "name": "la-haute-societe/craft-tarteaucitron",
  "description": "tarteaucitron.js integration for Craft CMS 3",
  "type": "craft-plugin",
  "version": "1.0.0",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "tarteaucitron",
    "rgpd",
    "gdpr"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "La Haute Société",
      "homepage": "https://www.lahautesociete.com"
    }
  ],
  "minimum-stability": "dev",
